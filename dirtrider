from js import robot

# robot.move_forward()      DirtRider'ı 1 birim ileri haraket ettirir.
# robot.move_backward()     DirtRider'ı 1 birim geri haraket ettirir.
# robot.turn_right()        DirtRider'ı 1 kez sağa döndürür.
# robot.turn_left()         DirtRider'ı 1 kez sola döndürür.
# robot.is_ok()             DirtRider'ın çalışıp çalışmadığını kontrol eder.

while robot.is_ok():
    robot.move_forward()
    robot.turn_right()
    robot.move_forward()
    robot.turn_right()
    robot.turn_right()
    robot.move_forward()
    robot.turn_right()
    robot.move_forward()
    robot.move_forward()
    robot.turn_right()